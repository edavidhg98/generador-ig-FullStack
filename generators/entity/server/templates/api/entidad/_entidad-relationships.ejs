<%_
  let result = '';
  let mongoosePopulates = [];
  for (let manyToOneRelationShip of manyToOneRelationShips) {
    let reference = `${manyToOneRelationShip.fieldName}: { type: Schema.Types.ObjectId, ref: '${manyToOneRelationShip.entityRef}', required: true }`;
    mongoosePopulates.push(reference);
  }
  result += mongoosePopulates.join(',');
_%>
<%- result %>
